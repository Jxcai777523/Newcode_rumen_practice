第11题：
#include <stdio.h>

int main() 
{
    long long int i;
    double a,b,c;
    scanf("%lld;",&i);
    scanf("%lf,%lf,%lf",&a,&b,&c);
    a = (int)((a*100)+0.5)/100.0;//进行四舍五入操作
    printf("The each subject score of No. %lld is %.2f, %.2f, %.2f.",i,a,b,c);
    return 0;
}
